        "ch_12_herons_formula": [
            {
                "id": "ex_heron_1",
                "question": "Find the area of a triangle with sides 7 cm, 8 cm, and 9 cm using Heron's formula",
                "steps": [
                    {
                        "explanation": "Calculate the semi-perimeter $s = \\frac{a + b + c}{2}$",
                        "math": "s = \\frac{7 + 8 + 9}{2} = \\frac{24}{2} = 12 \\text{ cm}"
                    },
                    {
                        "explanation": "Apply Heron's formula: $\\text{Area} = \\sqrt{s(s-a)(s-b)(s-c)}$",
                        "math": "\\text{Area} = \\sqrt{12(12-7)(12-8)(12-9)}"
                    },
                    {
                        "explanation": "Simplify",
                        "math": "= \\sqrt{12 \\times 5 \\times 4 \\times 3} = \\sqrt{720}"
                    },
                    {
                        "explanation": "Simplify the square root",
                        "math": "= \\sqrt{144 \\times 5} = 12\\sqrt{5}"
                    }
                ],
                "finalAnswer": "$12\\sqrt{5}$ cm$^2$ (approximately 26.83 cm$^2$)"
            }
        ],
        "ch_13_surface_areas_and_volumes": [
            {
                "id": "ex_sav_1",
                "question": "A cylindrical tank has a radius of 7 m and height of 10 m. Find its total surface area and volume. (Take $\\pi = \\frac{22}{7}$)",
                "steps": [
                    {
                        "explanation": "Total Surface Area of cylinder = $2\\pi r(r + h)$",
                        "math": "= 2 \\times \\frac{22}{7} \\times 7 \\times (7 + 10)"
                    },
                    {
                        "explanation": "Simplify",
                        "math": "= 2 \\times 22 \\times 17 = 748 \\text{ m}^2"
                    },
                    {
                        "explanation": "Volume of cylinder = $\\pi r^2 h$",
                        "math": "= \\frac{22}{7} \\times 7^2 \\times 10 = \\frac{22}{7} \\times 49 \\times 10"
                    },
                    {
                        "explanation": "Simplify",
                        "math": "= 22 \\times 7 \\times 10 = 1540 \\text{ m}^3"
                    }
                ],
                "finalAnswer": "Total Surface Area $= 748$ m$^2$, Volume $= 1540$ m$^3$"
            }
        ],
