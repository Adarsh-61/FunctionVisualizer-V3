        "ch_4_quadratic_equations": [
            {
                "id": "ex_qe_1",
                "question": "Solve the quadratic equation $2x^2 - 7x + 3 = 0$ using the quadratic formula",
                "steps": [
                    {
                        "explanation": "The quadratic formula is $x = \\frac{-b \\pm \\sqrt{b^2 - 4ac}}{2a}$"
                    },
                    {
                        "explanation": "Here $a = 2$, $b = -7$, $c = 3$. Calculate discriminant",
                        "math": "D = b^2 - 4ac = (-7)^2 - 4(2)(3) = 49 - 24 = 25"
                    },
                    {
                        "explanation": "Since $D > 0$, we have two distinct real roots",
                        "math": "x = \\frac{-(-7) \\pm \\sqrt{25}}{2(2)} = \\frac{7 \\pm 5}{4}"
                    },
                    {
                        "explanation": "Calculate both roots",
                        "math": "x_1 = \\frac{7 + 5}{4} = \\frac{12}{4} = 3 \\quad \\text{and} \\quad x_2 = \\frac{7 - 5}{4} = \\frac{2}{4} = \\frac{1}{2}"
                    }
                ],
                "finalAnswer": "$x = 3$ or $x = \\frac{1}{2}$"
            },
            {
                "id": "ex_qe_2",
                "question": "Find the nature of roots of $x^2 + 4x + 5 = 0$ without solving",
                "steps": [
                    {
                        "explanation": "Calculate the discriminant $D = b^2 - 4ac$"
                    },
                    {
                        "explanation": "Here $a = 1$, $b = 4$, $c = 5$",
                        "math": "D = (4)^2 - 4(1)(5) = 16 - 20 = -4"
                    },
                    {
                        "explanation": "Since $D < 0$, the equation has no real roots (complex roots)"
                    }
                ],
                "finalAnswer": "No real roots (two complex conjugate roots)"
            }
        ],
