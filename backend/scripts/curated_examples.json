        "ch_8_introduction_to_trigonometry": [
            {
                "id": "ex_trig_1",
                "question": "If $\\tan A = \\frac{3}{4}$, find $\\sin A$ and $\\cos A$ (where $A$ is an acute angle)",
                "steps": [
                    {
                        "explanation": "Given $\\tan A = \\frac{\\text{opposite}}{\\text{adjacent}} = \\frac{3}{4}$. So opposite = 3k, adjacent = 4k for some k"
                    },
                    {
                        "explanation": "Find hypotenuse using Pythagoras theorem",
                        "math": "\\text{hypotenuse} = \\sqrt{(3k)^2 + (4k)^2} = \\sqrt{9k^2 + 16k^2} = \\sqrt{25k^2} = 5k"
                    },
                    {
                        "explanation": "Calculate $\\sin A$ and $\\cos A$",
                        "math": "\\sin A = \\frac{\\text{opposite}}{\\text{hypotenuse}} = \\frac{3k}{5k} = \\frac{3}{5}"
                    },
                    {
                        "explanation": "Similarly",
                        "math": "\\cos A = \\frac{\\text{adjacent}}{\\text{hypotenuse}} = \\frac{4k}{5k} = \\frac{4}{5}"
                    }
                ],
                "finalAnswer": "$\\sin A = \\frac{3}{5}$, $\\cos A = \\frac{4}{5}$"
            },
            {
                "id": "ex_trig_2",
                "question": "Prove that: $\\frac{1 + \\tan^2 A}{1 + \\cot^2 A} = \\tan^2 A$",
                "steps": [
                    {
                        "explanation": "Use identities: $1 + \\tan^2 A = \\sec^2 A$ and $1 + \\cot^2 A = \\csc^2 A$",
                        "math": "\\text{LHS} = \\frac{\\sec^2 A}{\\csc^2 A}"
                    },
                    {
                        "explanation": "Convert to sin and cos",
                        "math": "= \\frac{1/\\cos^2 A}{1/\\sin^2 A} = \\frac{\\sin^2 A}{\\cos^2 A}"
                    },
                    {
                        "explanation": "Simplify",
                        "math": "= \\tan^2 A = \\text{RHS}"
                    }
                ],
                "finalAnswer": "Hence Proved"
            }
        ]
    },
