node_modules/
.pnp/
.pnp.js

# Build outputs (will be created during build)
.next/
out/
build/

# Testing
coverage/
.nyc_output/

# IDE
.idea/
.vscode/
*.swp
*.swo

# Environment files (SECURITY)
.env
.env.*
!.env.example

