            {
                "id": "ex_ns_1",
                "question": "Rationalize the denominator: $\\frac{1}{\\sqrt{2} - 1}$",
                "steps": [
                    {
                        "explanation": "Multiply numerator and denominator by the conjugate of the denominator, which is $(\\sqrt{2} + 1)$",
                        "math": "\\frac{1}{\\sqrt{2} - 1} \\times \\frac{\\sqrt{2} + 1}{\\sqrt{2} + 1}"
                    },
                    {
                        "explanation": "Apply the identity $(a-b)(a+b) = a^2 - b^2$ to the denominator",
                        "math": "= \\frac{\\sqrt{2} + 1}{(\\sqrt{2})^2 - (1)^2}"
                    },
                    {
                        "explanation": "Simplify the denominator: $2 - 1 = 1$",
                        "math": "= \\frac{\\sqrt{2} + 1}{1} = \\sqrt{2} + 1"
                    }
                ],
                "finalAnswer": "$\\sqrt{2} + 1$"
            },
            {
                "id": "ex_ns_2",
                "question": "Simplify: $2^{5} \\times 2^{-3} \\times 2^{2}$",
                "steps": [
                    {
                        "explanation": "Apply the law of exponents: $a^m \\times a^n = a^{m+n}$",
                        "math": "2^{5} \\times 2^{-3} \\times 2^{2} = 2^{5 + (-3) + 2}"
                    },
                    {
                        "explanation": "Add the exponents: $5 - 3 + 2 = 4$",
                        "math": "= 2^{4}"
                    },
                    {
                        "explanation": "Calculate the final value",
                        "math": "= 16"
                    }
                ],
                "finalAnswer": "$16$"
            },
            {
                "id": "ex_ns_3",
                "question": "Express $0.\\overline{6}$ in the form $\\frac{p}{q}$, where p and q are integers and $q \\neq 0$",
                "steps": [
                    {
                        "explanation": "Let $x = 0.\\overline{6} = 0.6666...$"
                    },
                    {
                        "explanation": "Multiply both sides by 10",
                        "math": "10x = 6.6666..."
                    },
                    {
                        "explanation": "Subtract the original equation from this",
                        "math": "10x - x = 6.6666... - 0.6666..."
                    },
                    {
                        "explanation": "Simplify to get",
                        "math": "9x = 6 \\implies x = \\frac{6}{9} = \\frac{2}{3}"
                    }
                ],
                "finalAnswer": "$\\frac{2}{3}$"
            }
